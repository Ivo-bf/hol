pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('check') {
            steps {
                echo 'check image'
            }
        }
        stage('commit') {
            steps {
                echo 'commit image'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy image'
            }
        }
        stage('docker') {
            steps {
                echo 'docker image'
            }
        }
    }
}


